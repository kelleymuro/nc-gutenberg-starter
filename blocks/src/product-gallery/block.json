{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "north-commerce/product-gallery",
  "version": "0.1.0",
  "title": "Product Gallery",
  "category": "north-commerce",
  "icon": "smiley",
  "description": "Product Gallery block.",
  "supports": {
    "html": true
  },
  "attributes": {
    "addToCartText": {
      "type": "string",
      "default": "Add to cart"
    },
    "category": {
      "type": "string",
      "default": "__all__"
    },
    "titleTextColor": {
      "type": "string",
      "default": "#000000"
    },
    "priceTextColor": {
      "type": "string",
      "default": "#000000"
    }
  },
  "textdomain": "north-commerce-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
